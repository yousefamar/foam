  <script>
    document.addEventListener("DOMContentLoaded", function() {
      const priceEl = document.querySelector("#price");
      const hoursEl = document.querySelector("#hours");
      const priceNoticeEl = document.querySelector("#price-notice");
      const priceNoticeTextEl = document.querySelector("#price-notice-text");

      function calculateRate() {
        const hoursPerWeek = parseInt(document.querySelector('input[type="range"]').value);
        let startupStage = 0;
        let techComplexity = 0;
        let underprivileged = document.querySelector('input[type="checkbox"]').checked ? 1 : 0;

        document.querySelectorAll(".startup-buttons button").forEach((button, index) => {
          if (button.classList.contains("bg-indigo-600")) {
            startupStage = index + 1;
          }
        });

        document.querySelectorAll(".tech-buttons button").forEach((button, index) => {
          if (button.classList.contains("bg-indigo-600")) {
            techComplexity = index + 1;
          }
        });

        let rate = 0
          + (hoursPerWeek * 5)
          + (startupStage * startupStage * startupStage * 5)
          + (techComplexity * techComplexity * techComplexity * 10);
        if (startupStage > 3)
          rate *= 2;
        if (underprivileged)
          rate /= 5;

        rate = Math.max(rate, 40);
        rate *= hoursPerWeek;

        priceEl.innerText = rate;

        if (hoursPerWeek > 4) {
          priceNoticeTextEl.innerText = "It's unlikely that you'll need more than 4 hours a week; I can do a LOT for you in less than that. Counterintuitively, my rate goes up with hours.";
          priceNoticeEl.classList.remove("hidden");
        } else if (startupStage < 2) {
          priceNoticeTextEl.innerText = "It may be too early for us to work together. If your idea is unvalidated, I can help you validate, but I won't let you burn your money to have an unvalidated product built!";
          priceNoticeEl.classList.remove("hidden");
        } else if (underprivileged) {
          priceNoticeTextEl.innerText = "I'm happy to work with underprivileged founders at a deeply discounted rate. I've been there and I know how hard it is to get started. Let's talk!";
          priceNoticeEl.classList.remove("hidden");
        } else {
          priceNoticeEl.classList.add("hidden");
        }
      }

      document.querySelector('input[type="range"]').addEventListener("input", function(e) {
        hoursEl.innerText = e.target.value;
        calculateRate();
      });

      const buttonGroups = ["startup-buttons", "tech-buttons"];
      
      buttonGroups.forEach((cls) => {
        document.querySelectorAll(`.${cls} button`).forEach((button) => {
          button.addEventListener("click", function(e) {
            document.querySelectorAll(`.${cls} button`).forEach((btn) => {
              btn.classList.remove("bg-indigo-600", "text-white");
            });
            e.currentTarget.classList.add("bg-indigo-600", "text-white");
            calculateRate();
          });
        });
      });

      document.querySelector('input[type="checkbox"]').addEventListener("change", calculateRate);

      document.querySelectorAll(".startup-buttons button")[1].classList.add("bg-indigo-600", "text-white");
      document.querySelectorAll(".tech-buttons button")[1].classList.add("bg-indigo-600", "text-white");

      calculateRate();
    });

